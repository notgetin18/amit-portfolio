{
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "key": "host",
          "value": "amitdevjourney.xyz"
        }
      ],
      "destination": "https://www.amitdevjourney.xyz/:path*",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "header",
          "key": "x-forwarded-proto",
          "value": "http"
        }
      ],
      "destination": "https://www.amitdevjourney.xyz/:path*",
      "permanent": true
    }
  ]
}